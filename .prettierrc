{
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true,
    "importOrder": ["^[./]"],
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
